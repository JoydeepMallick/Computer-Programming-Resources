#NO_badge_cont.NO_visible { visibility: visible; pointer-events: auto; }

#NO_badge { display: block; border-radius: 20px; background: rgb(220, 53, 69); height: 16px; width: 16px; color: rgb(255, 255, 255); text-align: center; line-height: 16px; font-size: 11px; cursor: pointer; position: absolute; top: 8px; left: 8px; opacity: 0; transform: scale(0); transition: all 0.3s ease 0s; }

#NO_badge.NO_softHidden { background: rgb(212, 212, 212); opacity: 1; transform: scale(0.6); }

#NO_badge.NO_hidden { opacity: 0; transform: scale(0); }

#NO_badge.NO_visible { animation: 0.3s ease 1s 1 normal forwards running animateIn; }

@-webkit-keyframes animateIn { 
  0% { opacity: 0; transform: scale(0); }
  100% { opacity: 1; transform: scale(0.7); }
}

@keyframes animateIn { 
  0% { opacity: 0; transform: scale(0); }
  100% { opacity: 1; transform: scale(0.7); }
}

#NO_frame_cont { pointer-events: none; border-radius: 4px; box-shadow: rgba(99, 114, 130, 0.32) 0px 0px 1px, rgba(27, 39, 51, 0.08) 0px 8px 16px; background: rgb(255, 255, 255); border: none; position: absolute; top: -900em; z-index: 999; width: 340px; height: 0px; opacity: 0; will-change: height, margin-top, opacity; margin-top: -10px; transition: margin-top 0.15s ease-out 0s, opacity 0.1s ease-out 0s, height 0.3s ease-out 0s; }

#NO_frame_cont.NO_visible { opacity: 1; pointer-events: auto; margin-top: 0px; }

#NO_frame { background: rgb(255, 255, 255); border: none; width: 100%; height: 100%; overflow: hidden; border-radius: 4px; position: relative; z-index: 900; transition: height 0.3s ease-out 0s; }

#NO_frame_cont.NO_embed { position: static; margin-top: 0px; }

#NO_badge.NO_softHidden { background: rgb(3, 169, 244) !important; box-shadow: rgb(24, 53, 76) 0px 0px 0px 3px !important; }

#NO_badge.NO_visible { font-size: 9px !important; height: 14px !important; width: 14px !important; line-height: 14px !important; font-family: "proxima nova" !important; box-shadow: rgb(24, 53, 76) 0px 0px 0px 3px !important; }
