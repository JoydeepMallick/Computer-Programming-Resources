<span class="k">values</span><span class="p">(</span><span class="s1">'John'</span><span class="p">,</span> <span class="mi">45</span><span class="p">);</span>

<span class="k">insert</span> <span class="k">into</span> <span class="n">customer</span><span class="p">(</span><span class="n">name</span><span class="p">,</span> <span class="n">age</span><span class="p">)</span>
<span class="k">values</span><span class="p">(</span><span class="s1">'Max'</span><span class="p">,</span> <span class="mi">25</span><span class="p">);</span>

<span class="k">insert</span> <span class="k">into</span> <span class="n">customer</span><span class="p">(</span><span class="n">name</span><span class="p">,</span> <span class="n">age</span><span class="p">)</span>
<span class="k">values</span><span class="p">(</span><span class="s1">'Jane'</span><span class="p">,</span> <span class="mi">20</span><span class="p">);</span>

<span class="k">insert</span> <span class="k">into</span> <span class="n">customer</span><span class="p">(</span><span class="n">name</span><span class="p">,</span> <span class="n">age</span><span class="p">)</span>
<span class="k">values</span><span class="p">(</span><span class="s1">'Bob'</span><span class="p">,</span> <span class="mi">34</span><span class="p">);</span>
