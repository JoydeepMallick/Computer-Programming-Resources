<span class="n">db</span> <span class="o">=</span> <span class="n">mysql</span><span class="o">.</span><span class="n">connector</span><span class="o">.</span><span class="n">connect</span><span class="p">(</span><span class="n">option_files</span><span class="o">=</span><span class="s1">'my.conf'</span><span class="p">,</span> <span class="n">use_pure</span><span class="o">=</span><span class="bp">True</span><span class="p">)</span>

<span class="n">cursor</span> <span class="o">=</span> <span class="n">db</span><span class="o">.</span><span class="n">cursor</span><span class="p">()</span>

