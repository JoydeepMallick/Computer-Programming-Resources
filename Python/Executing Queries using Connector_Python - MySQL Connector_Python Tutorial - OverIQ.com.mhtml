<span class="p">(</span><span class="s1">'BOL'</span><span class="p">,</span> <span class="s1">'Bolivia'</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">,</span> <span class="mi">8329000</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">)</span>
<span class="p">(</span><span class="s1">'BRA'</span><span class="p">,</span> <span class="s1">'Brazil'</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">,</span> <span class="mi">170115000</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">)</span>
<span class="p">(</span><span class="s1">'CHL'</span><span class="p">,</span> <span class="s1">'Chile'</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">,</span> <span class="mi">15211000</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">)</span>
<span class="p">(</span><span class="s1">'COL'</span><span class="p">,</span> <span class="s1">'Colombia'</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">,</span> <span class="mi">42321000</span><span class="p">,</span> <span class="s1">'South America'</span><span class="p">)</span>

<span class="n">Query</span> <span class="mi">3</span> <span class="o">-</span> <span class="n">select</span> <span class="mi">1</span><span class="o">+</span><span class="mi">1</span><span class="p">:</span>
<span class="p">(</span><span class="mi">2</span><span class="p">,)</span>
