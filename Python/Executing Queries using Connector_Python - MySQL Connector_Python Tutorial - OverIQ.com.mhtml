<span class="n">sql1</span> <span class="o">=</span> <span class="s2">"select version()"</span>

<span class="n">sql2</span> <span class="o">=</span> <span class="s2">"""select Code, Name, Region, Population, Continent from</span>
<span class="s2">country where Continent=</span><span class="si">%s</span><span class="s2"> limit </span><span class="si">%s</span><span class="s2">"""</span>

